<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Pskin.App">
    <Application.Resources>
        <ResourceDictionary>
            
            <Color x:Key="Accent">#F8B195</Color>
            <Color x:Key="Primary">#F67280</Color>
             <Color x:Key="Secondary">#355c7d</Color>
            <Color x:Key="PrimaryDark">#C06C84</Color>
            <Color x:Key="SecondaryDark">#6C5B7B</Color>
           
            <OnPlatform
            x:Key="Montserrat"
            x:TypeArguments="x:String"
            Android="Montserrat-Regular.ttf#Montserrat-Regular"
            iOS="Montserrat-Regular" />
            
            <OnPlatform
            x:Key="MontserratMedium"
            x:TypeArguments="x:String"
            Android="Montserrat-SemiBold.ttf#Montserrat-SemiBold"
            iOS="Montserrat-SemiBold" />
            
            <OnPlatform
            x:Key="Poppins"
            x:TypeArguments="x:String"
            Android="Poppins-Regular.ttf#Poppins-Regular"
            iOS="Poppins-Regular" />
            
            <OnPlatform
            x:Key="PoppinsBold"
            x:TypeArguments="x:String"
            Android="Poppins-SemiBold.ttf#Poppins-SemiBold"
            iOS="Poppins-SemiBold" />
            
        </ResourceDictionary>
    </Application.Resources>
</Application>